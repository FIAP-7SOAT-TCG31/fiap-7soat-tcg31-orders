{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@fiap-burger/setup/(.*)": "<rootDir>/../libs/setup/src/$1",
    "@fiap-burger/setup": "<rootDir>/../libs/setup/src",
    "@fiap-burger/tactical-design/(.*)": "<rootDir>/../libs/tactical-design/src/$1",
    "@fiap-burger/tactical-design": "<rootDir>/../libs/tactical-design/src"
  }
}